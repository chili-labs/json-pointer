{
    "name": "chili-labs/json-pointer",
    "description": "Simple library for accessing PHP data structures with JSON Pointers (RFC 6901)",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Tschinder",
            "email": "daniel@tschinder.de",
            "homepage": "http://www.tschinder.de"
        }
    ],
    "require": {
        "php": ">=5.3.3"
    },
    "require-dev": {
        "symfony/property-access": "~2.3"
    },
    "suggest": {
        "symfony/property-access": "When using the PropertyAccessAccessor you need to install this."
    },
    "autoload": {
        "psr-4": { "ChiliLabs\\JsonPointer\\": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
